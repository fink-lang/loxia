{parse} = import '@fink/larix'
{length} = import '@fink/std-lib/iter'

{generate} = import '..'


fink2js = fn source, options:
  filename = 'test.fnk'
  ast = parse source, filename

  match ast:
    {errors: 0 == length ?}:
      {code, errors} = generate ast, filename, source, options
      match errors:
        0 == length ?:
          code
        else:
          {errors}
    else:
      {errors: ast.errors}

