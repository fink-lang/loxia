{parse} = import '@fink/larix'
{is_empty} = import '@fink/std-lib/iter.fnk'

{generate} = import '../index.fnk'


fink2js = fn source, options:
  filename = 'test.fnk'
  ast = parse source, filename

  match ast:
    {errors: is_empty ?}:
      {code, errors} = generate ast, filename, source, options
      match errors:
        is_empty ?:
          code
        else:
          {errors}
    else:
      {errors: ast.errors}

