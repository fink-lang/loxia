{fink2js} = import '../../testing'
{describe, it, expect, to_match_snapshot} = import '@fink/jest'


describe 'partial', fn:
  it 'compiles assignment', fn:
    expect
      fink2js '
        foo1 = a * ? + 1
        foo2 = bar ?
        foo3 = ?.bar spam
        foo4 = ? ()
        foo5 = ? _
        foo6 = ? bar, spam
      '
      to_match_snapshot

    expect
      fink2js '
        foo1 = [1, ...?, 9]
        foo2 = {foo, ...?}
      '
      to_match_snapshot

