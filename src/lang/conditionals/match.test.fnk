{fink2js} = import '../../testing'
{describe, it, eq, snapshot} = import '../../testing/jest'


describe:: 'logical', fn:

  it:: 'compiles', fn:
    fink2js(`
      match shrub:
        {foo: 4, ni}: spam + ni
        123: spam
        [1, 2, ni]: ni
        else: shrub
    `) eq snapshot


  it:: 'compiles without else', fn:
    fink2js(`
      match shrub:
        {foo: 4, ni}: spam + ni
        123: spam
    `) eq snapshot


  it:: 'compiles nested', fn:
    fink2js(`
      match shrub:
        {foo: 4, ni: {na, nu}}: spam + ni
        {foo: {bar: 'spam'}}: ni
        [1, 2, [ni]]: ni
        123: spam
    `) eq snapshot

