{fink2js} = import '../../testing'
{describe, it, expect, to_match_snapshot} = import '@fink/jest'


describe 'filter', fn:
  it 'compiles', fn:
    expect
      fink2js `
        filter item:
          item % 2 == 0

        filter [a, ...b]:
          a_alone = a && b.length == 0
          a_alone

        filter [...a, b]:
          b && a.length == 0
      `
      to_match_snapshot

