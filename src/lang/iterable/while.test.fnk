{fink2js} = import '../../testing'
{describe, it, expect, to_match_snapshot} = import '@fink/jest'


describe 'while', fn:
  it 'compiles', fn:
    expect
      fink2js `
        while item:
          item < 10
      `
      to_match_snapshot


  it 'compiles destructuring', fn:
    expect
      fink2js `
        while [foo, ...bar]:
          foo_alone = foo and 0 == length bar
          foo_alone

        while [...foo, bar]:
          bar and 0 == length foo
      `
      to_match_snapshot

