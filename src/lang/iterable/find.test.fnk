{fink2js} = import '../../testing'
{describe, it, expect, to_match_snapshot} = import '@fink/jest'


describe 'find', fn:
  it 'compiles simple find', fn:
    expect
      fink2js`
        find item:
          item > 3
      `
      to_match_snapshot


  it 'compiles complex find', fn:
    expect
      fink2js`
        find {item}:
          match item:
            {spam}: shrub
            else: ni

        find [foo, ...bar]:
          foo_alone = foo && bar.length == 0
          foo_alone

        find [...foo, bar]:
          bar && foo.length == 0
      `
      to_match_snapshot




