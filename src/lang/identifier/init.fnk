babel_types = import '@babel/types'
{identifier} = babel_types

{escape_ident} = import '../../js/identifier.fnk'
{add, any} = import '../context.fnk'



transform_ident = fn {value}:
  name = escape_ident value
  identifier name



add_ident = fn ctx:
  pipe ctx:
    add 'ident', any, transform_ident
