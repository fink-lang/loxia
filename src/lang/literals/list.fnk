babel_types = import '@babel/types'
{arrayExpression} = babel_types
{null} = import '@fink/js-interop/nullish.fnk'



transform_list = fn node, {transform}:
  [...elems] = pipe node.exprs:
    map elem:
      match elem:
        {type: 'empty'}: null
        else: transform elem

  arrayExpression elems
