{numericLiteral} = import '@babel/types'
{rx, matches} = import '@fink/std-lib/regex'
{parse_float, parse_int} = import '@fink/std-lib/num'
{set_props} = import '@fink/std-lib/obj'


transform_number = fn {value}:
  match value:
    matches ?, rx'\.':
      numericLiteral parse_float value
    else:
      num = parse_int value
      js = numericLiteral num
      set_props js, {extra: {raw: value, rawValue: num}}


