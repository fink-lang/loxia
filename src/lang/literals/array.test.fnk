{fink2js} = import '../../testing'
{describe, it, eq, snapshot} = import '../../testing/jest'


describe:: 'array', fn:
  it:: 'compiles', fn:
    fink2js(`
      array1 = []
      array2 = [1]
      array3 = [1, 2, 4, a(), ...b]
      array4 = [
        a + 1 + 45+
          b + c,
        [1, 2],
        (3 + 3) * 2
      ]
    `) eq snapshot


describe:: 'destructuring array', fn:
  it:: 'compiles', fn:
    fink2js(`
      [a, b, c] = ni
      [,, d] = ni
      [head, ...tail] = ni
      [...items, last] = '1234'
      [first, second, ...middle, penultimate, end] = '123'
    `) eq snapshot



