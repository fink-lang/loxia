{fink2js} = import '../../testing/generate.fnk'
{describe, it, expect, to_match_snapshot} = import '@fink/jest/test.fnk'


describe 'numbers', fn:
  it 'transforms integers', fn:
    expect
      fink2js '
        x = 1234578
        y = 0123
      '
      to_match_snapshot


  it 'transforms floats', fn:
    expect
      fink2js '
        x = 1.234578
        y = 1.23e45
        z = 1.23e-45
        a = 1.23e+45
      '
      to_match_snapshot


  it 'transforms hex, octet, binary', fn:
    expect
      fink2js '
        h = 0x123456789ABCDEF0
        o = 0o12345670
        b = 0b01010
      '
      to_match_snapshot


