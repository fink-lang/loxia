{binaryExpression} = import '@babel/types'


transform_op = {
  '==': '===',
  '!=': '!==',
  '^': '**'
}


transform_binary = fn {op, left, right}, {transform}:
  {(op): operator=op} = transform_op

  bin_left = transform:: left
  bin_right = transform:: right

  binaryExpression:: operator, bin_left, bin_right
