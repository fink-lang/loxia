{fink2js} = import '../../testing'
{describe, it, expect, to_match_snapshot} = import '@fink/jest'


describe 'throw', fn:
  it 'compiles', fn:
    expect
      fink2js`
        fn bar:
          match bar:
            has_err ?: throw err 'spam'
            else: bar
      `
      to_match_snapshot


