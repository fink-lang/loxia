{fink2js} = import '../../testing'
{describe, it, expect, to_match_snapshot} = import '@fink/jest'


describe 'module', fn:
  it 'handles comments', fn:

    expect
      fink2js "
        #!/usr/bin/env node

        # leading comment

        # imp comment
        import 'shrub'

        # imp comment 2
        foo = import './foo'

        # imp comment 3
        {bar} = import './spam'
        {foo: spam} = import './shrub'

        # expr comment
        shrub = foo + bar
        ni = await import spam

        # trailing comment
      "
      to_match_snapshot



