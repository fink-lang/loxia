{fink2js} = import '../../testing'
{describe, it, expect, to_match_snapshot} = import '@fink/jest'


describe 'await', fn:
  it 'compiles', fn:

    expect
      fink2js `
        task1 = fn foo: - await foo

        task2 = fn foo: await (foo + 4)

        task3 = fn foo:
          bar = await foo ()
          spam = await foo _
          bar + 123

        a_gen = unfold curr=0:
          match shrub:
            spam: await ni curr
            else: curr + 1

        await ni
      `
      to_match_snapshot



