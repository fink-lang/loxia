{describe, it, expect, to_match_snapshot} = import '@fink/jest/test.fnk'
{fink2js} = import '../../testing/generate.fnk'


describe 'module', fn:
  it 'handles directives', fn:
    expect
      fink2js "
        #!/usr/bin/env node

        foo = 1234
      "
      to_match_snapshot


  it 'handles exports', fn:
    expect
      fink2js "
        delete = fn: true
        Ï€ = fn: true
        foo_bar = 1234
        foo-bar = 1234
      "
      to_match_snapshot


  it 'handles side effects', fn:
    expect
      fink2js "
        foo bar
        shrub.ni foo
        spam = ni
      "
      to_match_snapshot