{fink2js} = import '../../testing/generate.fnk'
{skip, describe, it, expect, to_match_snapshot} = import '@fink/jest/test.fnk'


describe 'list', fn:
  it 'compiles', fn:
    expect
      fink2js '
        array1 = []
        array2 = [1]
        array3 = [1, 2]
        array4 = [1, , 2]
        array5 = list:
          a + 1 + 45 +
            b + c
          [1, 2]
          (3 + 3) * 2
      '
      to_match_snapshot


  it 'compiles spread', fn:
    expect
      fink2js '
        foo = [...b]
        bar = [1, 2, ...b]
        spam = [1, 2, ...b, 123]
      '
      to_match_snapshot

    expect
      fink2js '
        foo = [1, 2, 3]
        bar = [...foo, 4, 5, 6]
        spam = [7]
        shrub = [...bar, ...spam]
        ni = [...nu, na]
        ham = [...nu, ...na]
      '
      to_match_snapshot


  skip.it 'compiles as partial', fn:
    expect
      fink2js '
        [1, ...?, 9]
        [?, ?]
      '
      to_match_snapshot


describe 'unpacking list', fn:
  it 'destructures simple', fn:
    expect
      fink2js '
        [a, [b, c]] = [1, [2, 3]]
        foo = [b, a]
        [z] = [1 + 2, 3 * 4]
        [x, y] = foo
        out = [a, b, c, foo, z, x, y]
      '
      to_match_snapshot

  it 'destructures empty', fn:
    expect
      fink2js '
        [a,  , b] = foo
        out = [a, b]
      '
      to_match_snapshot

  it 'destructures spread', fn:
    expect
      fink2js '
        [a, ..., b] = foo
        out = [a, b]
      '
      to_match_snapshot

    expect
      fink2js '
        [a, ..., b, c] = foo
        out = [a, b, c]
      '
      to_match_snapshot

    expect
      fink2js '
        [a, ...b] = foo
        out = [a, ...b]
      '
      to_match_snapshot

    expect
      fink2js '
        [a, ...b, c] = foo
        out = [a, b, c]
      '
      to_match_snapshot


  it 'destructures recs', fn:
    expect
      fink2js "
        [{a, b}, c] = ni
        out = [a, b, c]
      "
      to_match_snapshot


  it 'destructures with defaults', fn:
    expect
      fink2js '
        [a=12, b=34] = foo
        out = [a, b]
      '
      to_match_snapshot

    expect
      fink2js '
        [[a, b=0]=[1, 2], c=34] = foo
        out = [a, b, c]
      '
      to_match_snapshot
